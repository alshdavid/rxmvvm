{
  "name": "rxmvvm",
  "version": "0.0.2",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "dev": "npx tsc --watch"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "tsconfig.json"
  ],
  "exports": {
    ".": {
      "rxmvvm:source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./reactive": {
      "rxmvvm:source": "./src/reactive/index.ts",
      "types": "./dist/reactive/index.d.ts",
      "default": "./dist/reactive/index.js"
    },
    "./preact": {
      "rxmvvm:source": "./src/preact/index.ts",
      "types": "./dist/preact/index.d.ts",
      "default": "./dist/preact/index.js"
    },
    "./preact/forms": {
      "rxmvvm:source": "./src/preact/forms/index.ts",
      "types": "./dist/preact/forms/index.d.ts",
      "default": "./dist/preact/forms/index.js"
    },
    "./react": {
      "rxmvvm:source": "./src/react/index.ts",
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    }
  }
}
